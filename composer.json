{
  "name": "zeroc0d3lab/commit-checker",
  "description": "Check coding standard & code syntax with Git pre-commit hook.",
  "type": "package",
  "license": "MIT",
  "keywords": [
    "zeroc0d3lab",
    "zeroc0d3lab cms",
    "zeroc0d3lab platform",
    "zeroc0d3lab git commit checker"
  ],
  "homepage": "https://zeroc0d3lab.com",
  "authors": [
    {
      "name": "ZeroC0D3Lab",
      "email": "zeroc0d3.team@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Zerocod3lab\\CommitChecker\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Zerocod3lab\\CommitChecker\\Tests\\": "tests"
    }
  },
  "require": {
    "php": "^7.1.3",
    "laravel/framework": "^5.6|^6.0",
    "jakub-onderka/php-parallel-lint": "dev-master",
    "squizlabs/php_codesniffer": "^3.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.1|^7.0|^8.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Zerocod3lab\\CommitChecker\\Providers\\CommitCheckerServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "stable"
}
